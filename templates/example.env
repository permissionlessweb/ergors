# Environment variables for Agent Orchestrator

# LLM Provider API Keys
AKASH_API_KEY=your_akash_api_key_here
KIMI_API_KEY=your_kimi_api_key_here
GROK_API_KEY=your_grok_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Database Configuration
REDIS_URL=redis://localhost:6379
POSTGRES_URL=postgresql://postgres:password@localhost:5432/agent_orchestrator

# Ollama Configuration (for local fallback)
OLLAMA_PRIMARY_HOST=localhost:11434
OLLAMA_SECONDARY_HOST=localhost:11435

# Orchestrator Configuration
MAX_CONCURRENT_TASKS=10
WORKSPACE_BASE_PATH=/tmp/agent-workspaces
DEFAULT_TASK_TIMEOUT=3600
LOG_LEVEL=INFO

# Docker Configuration
DOCKER_HOST=unix:///var/run/docker.sock
DEFAULT_MEMORY_LIMIT=1g
DEFAULT_CPU_COUNT=2

# Security
SECRET_KEY=your_secret_key_here
ALLOWED_HOSTS=localhost,127.0.0.1
CORS_ORIGINS=http://localhost:3000,http://localhost:8080

# Monitoring (optional)
PROMETHEUS_ENABLED=true
GRAFANA_ENABLED=true